{
  "name": "rdtsc",
  "version": "2.0.0",
  "description": "The most high resolution cross platform timing in NodeJs that use processor command RDTSC. Calculate performance with accuracy +/- 4 processor cycles.",
  "engines": {
    "node": ">= 12.4.0"
  },
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --plugin markdown --ext js,md .",
    "prepublish": "npm run precommit",
    "precommit": "npm run lint && npm run build && npm run test",
    "build": "node-gyp configure && node-gyp build",
    "test": "mocha",
    "watch": "mocha --watch --watch-extensions .*",
    "ci": "npm run lint && nyc npm run test && ncat tmp/coverage/all/lcov/**/lcov.info --banner \"\" | coveralls"
  },
  "keywords": [
    "native",
    "high-resolution",
    "performance",
    "timing",
    "rdtsc",
    "benchmark",
    "processor-cycles",
    "tick",
    "stopwatch",
    "duration",
    "thread-priority",
    "process-priority",
    "cross-platform"
  ],
  "bugs": {
    "url": "https://github.com/NikolayMakhonin/nodejs-rdtsc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:NikolayMakhonin/nodejs-rdtsc.git"
  },
  "author": {
    "name": "Nikolay Makhonin",
    "url": "https://github.com/NikolayMakhonin/"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^5.2.0",
    "ncat": "^2.0.7",
    "node-gyp": "^5.0.3",
    "nyc": "^14.1.1",
    "wavefile": "^8.4.5"
  },
  "dependencies": {}
}
